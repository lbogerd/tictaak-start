{
	"name": "tictaak-start",
	"private": true,
	"type": "module",
	"version": "0.1.0",
	"engines": {
		"node": ">=24"
	},
	"scripts": {
		"dev": "vite dev --port 3000",
		"start": "node .output/server/index.mjs",
		"prebuild": "bash scripts/git-sha-to-public.sh",
		"build": "vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"fix": "pnpx biome lint --write --unsafe && pnpx biome format --write",
		"comp:add": "pnpx shadcn@latest add",
		"db:generate": "pnpx drizzle-kit generate",
		"db:push": "pnpx drizzle-kit push",
		"db:seed": "node --experimental-strip-types src/lib/seed.ts",
		"db:reset": "pnpm run db:push && pnpm run db:seed"
	},
	"dependencies": {
		"@fontsource/poppins": "5.2.6",
		"@radix-ui/react-popover": "1.1.14",
		"@radix-ui/react-select": "2.2.5",
		"@radix-ui/react-separator": "1.1.7",
		"@radix-ui/react-slot": "1.2.3",
		"@radix-ui/react-tabs": "1.1.12",
		"@t3-oss/env-core": "0.13.8",
		"@tailwindcss/vite": "4.1.11",
		"@tanstack/react-router": "1.125.4",
		"@tanstack/react-router-devtools": "1.125.4",
		"@tanstack/react-router-with-query": "1.125.4",
		"@tanstack/react-start": "1.125.4",
		"@tanstack/router-plugin": "1.125.4",
		"clsx": "2.1.1",
		"date-fns": "4.1.0",
		"drizzle-orm": "0.44.3",
		"lucide-react": "0.525.0",
		"pg": "8.16.3",
		"react": "19.1.0",
		"react-day-picker": "9.8.0",
		"react-dom": "19.1.0",
		"tailwind-merge": "3.3.1",
		"tailwind-variants": "1.0.0",
		"tailwindcss": "4.1.11",
		"tailwindcss-animate": "1.0.7",
		"vite": "7.0.2",
		"vite-tsconfig-paths": "5.1.4",
		"zod": "3.25.74"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@electric-sql/pglite": "0.3.5",
		"@testing-library/dom": "10.4.0",
		"@testing-library/react": "16.3.0",
		"@types/pg": "8.15.4",
		"@types/react": "19.1.8",
		"@types/react-dom": "19.1.6",
		"@vitejs/plugin-react": "4.6.0",
		"drizzle-kit": "0.31.4",
		"fs-extra": "11.3.0",
		"husky": "9.1.7",
		"typescript": "5.8.3",
		"vite": "6.1.0",
		"vitest": "3.2.4",
		"web-vitals": "5.0.3"
	},
	"packageManager": "pnpm@10.13.1"
}
