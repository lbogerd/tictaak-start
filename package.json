{
	"name": "tictaak-start",
	"private": true,
	"type": "module",
	"version": "0.1.0",
	"engines": {
		"node": ">=24.6.0"
	},
	"scripts": {
		"dev": "vite dev --port 3000",
		"start": "node .output/server/index.mjs",
		"prebuild": "bash scripts/git-sha-to-public.sh",
		"build": "vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"fix": "pnpm exec biome lint --write --unsafe && pnpm exec biome format --write",
		"comp:add": "pnpm exec shadcn@latest add",
		"db:generate": "pnpm exec drizzle-kit generate",
		"db:push": "pnpm exec drizzle-kit push",
		"db:seed": "node src/lib/seed.ts",
		"db:reset": "pnpm run db:push && pnpm run db:seed",
		"db:studio": "pnpm exec drizzle-kit studio"
	},
