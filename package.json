		"clsx": "2.1.1",
		"date-fns": "4.1.0",
		"drizzle-orm": "0.44.4",
		"lucide-react": "0.540.0",
		"node-thermal-printer": "^4.5.0",
		"pg": "8.16.3",
		"pino": "^10.1.1",
		"pino-pretty": "^13.1.3",
